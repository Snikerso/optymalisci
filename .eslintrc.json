{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended"
    // "plugin:prettier/recommended",
    // "google",
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react-in-jsx-scope": "off",
    "react/prop-types": "off"
  }
}
